<h2>Add A New Gantt Chart</h2>

<br>

<form class="form-horizontal" role="form" action="/gantts" method="post">

  <div class="form-group">
    <label class="col-sm-2 control-label">Gantt URL</label>
    <div class="col-sm-10">
      <input type="text" class="form-control" name="url" placeholder="Some Address">
    </div>
  </div>

  <div class="form-group">
    <label class="col-sm-2 control-label">Project ID</label>
    <div class="col-sm-10">
      <input type="text" class="form-control" name="project" placeholder="Project ID">
    </div>
  </div>

  <div class="form-group">
    <div class="col-sm-offset-2 col-sm-10">
      <button type="submit" class="btn btn-default"> Submit </button> or <a href="/">Cancel</a>
    </div>
  </div>

</form>



